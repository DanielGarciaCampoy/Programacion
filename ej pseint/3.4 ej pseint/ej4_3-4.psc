Algoritmo ej4_3_4
	Definir n1, n2, suma, resta, multipl, division Como Real;
	Escribir "Introduzca n1: ";
		Leer n1;
	Escribir "Introduzca n2: ";
		Leer n2;
	suma <- n1 + n2;
		Escribir "Suma: ", suma;
	resta <- n1 - n2;
		Escribir "Resta: ", resta;
	multipl <- n1 * n2;
		Escribir "Multiplicación: ", multipl;
	division <- n1 / n2;
		Escribir "División: ", division;
	FinAlgoritmo