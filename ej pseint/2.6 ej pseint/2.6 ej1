Algoritmo variables_ej1
	Definir x,z Como Entero;
	Definir suma, resta, multiplicacion, division Como Real;
	x <- 144;
	z <- 999;
	//Suma, resta multiplicación y división
	suma <- x + z;
	resta <- x - z;
	multiplicacion <- x * z;
	division <- x / z;
	//resultados
	Escribir "valor de x: ", x;
	Escribir "valor de z: ", z;
	Escribir "suma: ", suma;
	Escribir "resta: ", resta;
	Escribir "division: ", division;
	Escribir "multiplicacion: ", multiplicacion;
FinAlgoritmo