Algoritmo _4_5_ej1
	Definir dia Como Caracter;
	
	Escribir "Día de la semana: ";
		Leer dia;
	
		Si dia = "lunes" Entonces;
			Escribir "Entornos de desarrollo";
		FinSi;
		Si dia = "martes" Entonces;
			Escribir "Programación";
		FinSi;
		Si dia = "miercoles" Entonces;
			Escribir "Programación";
		FinSi;
		Si dia = "jueves" Entonces;
			Escribir "Programación";
		FinSi;
		Si dia = "viernes" Entonces;
			Escribir "Formación y Orientación Laboral";
		FinSi;
FinAlgoritmo